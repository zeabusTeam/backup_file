<?xml version="1.0"?>
<launch>
	<arg name="mission"/>
	<rosparam ns="color_range_$(arg mission)" command="load" file="$(find zeabus_example)/params/morning/color_$(arg mission).yaml"></rosparam>
	<node pkg="zeabus_example" name="color_range" type="color_range_example.py" output="screen">
        <param name="cameraTopic" type="str" value="/top/center/image_raw/compressed"/>
		<param name="mission" type="str" value="$(arg mission)"/>
		<param name="time" type="str" value="morning"/>
	</node>
</launch>
